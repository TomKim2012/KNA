<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">


	<m:MaterialRow>
		<m:MaterialModalContent>
			<h5>Task Creation</h5>
			

			<m:MaterialRow>
				<m:MaterialColumn grid="s8">
					<m:MaterialAutoComplete ui:field="autoCompleteUsers"
						placeholder="Type Users Name" />
				</m:MaterialColumn>

				<m:MaterialColumn grid="s4">
					<m:MaterialDropDown text="Selection" color="blue"
						waves="light" divider="true" activates="myDropDown2">
						<m:item>
							<m:MaterialLink text="Video" iconPosition="right"
								icon="mdi-av-videocam" />
						</m:item>
						<m:item>
							<m:MaterialLink text="Photograph" iconPosition="right"
								icon="mdi-image-collections" />
						</m:item>
						<m:item>
							<m:MaterialLink text="Article" iconPosition="right"
								icon="mdi-action-assignment" />
						</m:item>
					</m:MaterialDropDown>
				</m:MaterialColumn>
				
			</m:MaterialRow>

			<m:MaterialRow>

			</m:MaterialRow>
		</m:MaterialModalContent>

		<g:HTMLPanel styleName="modal-footer">
			<div class="right">
				<g:Anchor text="Save"
					styleName="modal-action modal-close waves-effect waves-green btn-flat"></g:Anchor>
			</div>
		</g:HTMLPanel>

	</m:MaterialRow>
</ui:UiBinder> 